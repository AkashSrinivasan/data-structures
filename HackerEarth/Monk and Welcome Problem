#include<stdio.h>
int main(){
    int n,a[100000],b[100000],i;
    scanf("%d",&n);
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++){
        scanf("%d",&b[i]);
    }
    for(i=0;i<n;i++){
        a[i]=a[i]+b[i];
    }
    for(i=0;i<n;i++)
    {
        printf("%d ",a[i]);
    }
}
