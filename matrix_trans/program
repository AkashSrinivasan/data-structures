#include<stdio.h>
int main()
{
    int a[10][10],p[10][10],i,j,k,m,n;
    printf("enter the rows and coloumn");
    scanf("%d%d",&m,&n);
    printf("first");
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
            scanf("%d",&a[i][j]);
        }
    }
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
            p[j][i]=a[i][j];
        }
    }
    printf("ans\n");
    for(i=0;i<n;i++)
    {
        for(j=0;j<m;j++)
        {
            printf("%d ",p[i][j]);
        }
        printf("\n");
    }
}
