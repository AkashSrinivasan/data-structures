#include <stdio.h>
#include <stdlib.h>

int main()
{
    int *a,*b,i,j,k,l,n,save;
    printf("enter the elements=");
    scanf("%d",&n);
    a=(int*)malloc(n* sizeof(int));
    b=(int*)malloc(n* sizeof(int));
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    int first=0;
    int last=n-1;
    int f=true;
    for(i=0;i<n;i++)
    {
        if(f)
        {
            b[i]=a[last];
            last--;
        }
        else
        {
            b[i]=a[first];
            first++;
        }
        f=!f;
    }
    for(i=0;i<n;i++)
    {
        a[i]=b[i];
        printf("%d ",a[i]);
    }
    
    
    
    return 0;
}
